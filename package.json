{
  "name": "sinuous",
  "private": true,
  "version": "0.0.0",
  "description": "üêç Sinuous - Reactive render engine",
  "workspaces": {
    "packages": [
      "packages/*",
      "fixtures/*"
    ]
  },
  "scripts": {
    "lint": "eslint 'packages/{*,*/*}/src/**/*.js'",
    "format": "prettier --write 'packages/**/{src,test}/**/*.js' 'scripts/**/*.js' '**/*.md' '!**/build/**'",
    "test": "karma start",
    "coverage": "codecov",
    "build": "rollup -c scripts/rollup/config.js --silent",
    "watch": "rollup -wc scripts/rollup/config.js --silent"
  },
  "repository": "luwes/sinuous",
  "keywords": [
    "functional",
    "reactive",
    "declarative"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/sinuous/issues"
  },
  "homepage": "https://github.com/luwes/sinuous#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.17.2",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "minimist": "^1.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "puppeteer": "^1.14.0",
    "ramda": "^0.26.1",
    "rollup": "1.10.1",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-size": "^0.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}

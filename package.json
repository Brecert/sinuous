{
  "name": "sinuous",
  "private": true,
  "version": "0.0.0",
  "description": "ðŸ§¬ Small, fast, reactive render engine",
  "workspaces": {
    "packages": [
      "packages/*",
      "site/content/**/*"
    ]
  },
  "scripts": {
    "lint": "eslint 'packages/{*,*/*,}/src/**/*.js'",
    "format": "prettier --write 'packages/**/{src,test}/**/*.js' 'scripts/**/*.js' '**/*.md' '!**/build/**' '!**/tape/**' '!**/htm/**' '!**/babel-plugin-htm/**' '!**/site/**'",
    "build-tape": "browserify scripts/tape/src.js --standalone tape -o scripts/tape/dist.js",
    "test": "cross-env COVERAGE=true karma start",
    "coverage": "codecov",
    "postcoverage": "camo-purge",
    "build": "rollup -c scripts/rollup/config.js --silent",
    "watch": "rollup -wc scripts/rollup/config.js --silent",
    "serve": "(cd site && hugo serve --ignoreCache)",
    "start": "run-p watch serve",
    "docs": "node scripts/docs.js && yarn format:docs",
    "format:docs": "prettier --write '**/*.md'"
  },
  "repository": "luwes/sinuous",
  "keywords": [
    "functional",
    "reactive",
    "declarative"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/sinuous/issues"
  },
  "homepage": "https://github.com/luwes/sinuous#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "ansi-colors": "^4.1.1",
    "babel-eslint": "^10.0.2",
    "browserify": "^16.3.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.5.5",
    "faucet": "^0.0.1",
    "fs-extra": "^8.1.0",
    "jsdoc-to-markdown": "^5.0.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^1.1.2",
    "karma-rollup-preprocessor": "^7.0.2",
    "karma-sauce-launcher": "^1",
    "karma-sinon": "^1.0.5",
    "karma-tap": "^4.1.4",
    "karma-tap-pretty-reporter": "^4.1.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "observable": "^2.1.4",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "rollup": "1.17.0",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size": "^0.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "sinon": "^7.3.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  }
}
